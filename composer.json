{
    "name": "spinen/laravel-garbage-man",
    "description": "Scheduled job to clean out Laravel's soft deleted records at configured interval.",
    "keywords": [
        "softdelete",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/console": "~5.1.10|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/database": "~5.1.10|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "~5.1.10|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "nesbot/carbon": "~1.19",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "^1",
        "symfony/thanks": "^1.0",
        "symfony/var-dumper": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\GarbageMan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\GarbageMan\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\GarbageMan\\GarbageManServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
